{
    "version": 2,
    "public": true,
    "builds": [
      { "src": "public/index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/sitemap.xml", "dest": "/public/sitemap.xml" },
      { "src": "/robots.txt", "dest": "/public/robots.txt" },
      { "src": "/(.*)", "dest": "/public/$1" },
      { "handle": "filesystem" },
      { "src": "/", "dest": "/public/index.html" }
    ]
  }
  
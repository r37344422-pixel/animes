{
    "version": 2,
    "builds": [
      { "src": "**/*.html", "use": "@vercel/static" },
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/sitemap.xml", "dest": "public/sitemap.xml" },
      { "src": "/robots.txt", "dest": "public/robots.txt" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  